{
  "name": "dinamo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "SET NODE_ENV=production & nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "init": "npm install && npm i -g swagger",
    "swagger": "concurrently \"json-server --watch api/json/db.json --port 4200\" \"swagger project edit -p 3600\"",
    "story-book": "npx nuxt storybook",
    "create-component": "bash createComponent.sh",
    "all-serve": "concurrently \"nuxt\" \"json-server --watch api/json/db.json --port 4200\" \"swagger project edit -p 3600\""
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "@storybook/vue": "^6.1.21",
    "concurrently": "^6.0.0",
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "^3.8.3",
    "eventsource-polyfill": "^0.9.6",
    "json-server": "^0.16.3",
    "nuxt": "^2.14.12",
    "nuxt-i18n": "^6.21.1",
    "portal-vue": "^2.1.7",
    "pug": "^3.0.2",
    "pug-plain-loader": "^1.1.0",
    "scroll-lock": "^2.1.5",
    "storybook": "^6.2.1",
    "swiper": "^5.4.5",
    "v-click-outside": "^3.1.2",
    "v-mask": "^2.2.4",
    "v-tooltip": "^2.1.3",
    "vue-awesome-swiper": "^4.1.1",
    "vue-multiselect": "^2.1.6",
    "vue-plyr": "^7.0.0",
    "vue-range-component": "^1.0.3",
    "vue-slick-carousel": "^1.0.6",
    "vue-zoom-on-hover": "^1.0.5",
    "vuelidate": "^0.7.6",
    "vuescroll": "^4.17.3"
  },
